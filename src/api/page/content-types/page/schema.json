      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "subcategory": {
      "type": "enumeration",
      "enum": [
        "about-me",
        "treatments",
        "general",
        "implant-treatments",
        "oral-and-maxillofacial-surgery",
        "periodontology"
      ],
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
